page{
  height: 100%;
  width: 750rpx;
  background-color: #f2f2f2;
  display: flex;
  flex-direction: column;
}
#bodyContainer{
  /* Body will take up the rest of the space apart from the navbar and tabbar */
  flex: 1 0 10px;
  background-color: white;
  display: flex;
  flex-direction: column;
  width: 750rpx;
  overflow: auto;
}
.itemCard{
  height: auto;
  width: 100%;
  border-radius: 10px;
}

#bodyRow{
  display: flex;
  flex-direction: column;
}